<%= form_for @user do |f|%>
<%= f.label "Name"%>
<%=f.text_area :name %>
<%= f.label "Height (Inches)"%>
<%=f.text_area :height %>
<%= f.label "Happiness on scale of 1-5"%>
<%=f.text_area :happiness %>
<%= f.label "Nauseous on scale of 1-5"%>
<%=f.text_area :nausea %>
<%= f.label "How many dollars do you have?"%>
<%=f.text_area :tickets %>
<%=f.label "Check if you own a theme park"%>
<%=f.check_box :admin %>
<%= f.label "Password"%>
<%=f.text_area :password %>
<%=f.submit "Create User"%>
<%end%>
